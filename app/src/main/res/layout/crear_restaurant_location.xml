<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".crear_restaurant">


    <fragment
        android:id="@+id/map_frat"
        android:name="com.developer.david.apprestaurant.FragmentMaps"
        android:layout_width="402dp"
        android:layout_height="610dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtubicacion"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnregistrarlocation"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Registrar"
        app:layout_constraintBottom_toTopOf="@+id/map_frat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtubicacion"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtubicacion"
        android:layout_width="233dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:text="Ubicacion"
        app:layout_constraintHorizontal_bias="0.011"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>